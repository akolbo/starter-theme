                    <div class="grid__column bg-color-darker inverse">1</div>
                    <div class="grid__column bg-color-darker inverse">1</div>
                </div>
    
                <div class="grid-example grid padding-xxxs padding-y">
                    <div class="grid__column grid__column--8 bg-color-darker inverse">8</div>
